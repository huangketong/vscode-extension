{
	"name": "mamba",
	"displayName": "mamba",
	"description": "vscode extension for deploy and release with yml",
	"version": "0.0.9",
	"publisher": "ketong",
	"author": {
		"name": "ketong"
	},
	"repository": {
    "type": "git",
    "url": "https://github.com/mamba-1024/vscode-extension.git"
  },
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:customFrontend",
		"onView:sidebar_mamba"
	],
	"main": "./dist/extension.js",
	"icon": "images/icon.png",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "sidebar_yml",
					"title": "Mamba",
					"icon": "images/icon.svg"
				}
			]
		},
		"views": {
      "sidebar_yml": [
        {
          "id": "sidebar_mamba",
          "name": "dice_yml",
					"icon": "images/icon.svg"
        }
      ]
		},
		"commands": [
			{
				"command": "customFrontend",
				"title": "Custom Frontend"
			},
			{
				"command": "sidebar_mamba.deployEntry",
				"title": "Deploy",
				"icon": {
					"light": "resources/light/boolean.svg",
					"dark": "resources/dark/boolean.svg"
				}
			},
			{
				"command": "sidebar_mamba.releaseEntry",
				"title": "Release",
				"icon": {
					"light": "resources/light/dependency.svg",
					"dark": "resources/dark/dependency.svg"
				}
			},
			{
				"command": "sidebar_mamba.refreshYml",
				"title": "refreshYml",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "sidebar_mamba.refreshYml",
					"when": "view == sidebar_mamba",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "sidebar_mamba.deployEntry",
					"when": "view == sidebar_mamba && viewItem == yml",
					"group": "inline"
				},
				{
					"command": "sidebar_mamba.releaseEntry",
					"when": "view == sidebar_mamba && viewItem == yml",
					"group": "inline"
				},
				{
					"command": "sidebar_mamba.deployEntry",
					"when": "view == sidebar_mamba && viewItem == yml"
				},
				{
					"command": "sidebar_mamba.releaseEntry",
					"when": "view == sidebar_mamba && viewItem == yml"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"dep-package": "vsce package",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"ts-loader": "^9.2.8",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2",
		"@vscode/test-electron": "^2.1.3"
	},
	"dependencies": {
		"@babel/generator": "^7.18.0",
		"@babel/parser": "^7.18.0",
		"@babel/traverse": "^7.18.0"
	}
}
